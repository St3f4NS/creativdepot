{
	"autoprefixerBrowsers": [
		"last 2 versions",
		"> 5%"
	],
	"gzip_options": {
		"threshold": "1kb",
		"gzipOptions": {
			"level": 9
		}
	},
	"sourcePaths": {
		"scss": "src/scss/**/*.scss",
		"js": [
			"src/js/vendor/jquery-2.2.2.js",
			"src/js/*.js"
		],
		"php": [
			"dist/**/*.php"
		],
		"svg": [
			"src/images/*.svg"
		]
	},
	"deployPaths": {
		"scss": "dist/css",
		"js": "dist/js",
		"svg": "dist/images"
	},
	"deployFiles": {
		"scss": "main.css",
		"js": "main.js"
	},
	"cleanFiles": [
		"dist/css/main*.css",
		"dist/js/main.min*.js"
	],
	"proxy": "creativdepot.loc"
}
